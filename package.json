{
  "name": "statemint",
  "type": "commonjs",
  "version": "1.0.0",
  "description": "TypeScript based cloud resource manager",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
  },
  "bin": {
    "stratoform": "./dist/runtime/cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "ts-node -P tsconfig.build.json src/index.ts",
    "test:compile": "tsc -p tsconfig.test.json",
    "test:run": "node dist-tests/tests/deployment.stratoform.js",
    "test": "npm run test:compile && npm run test:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KR-Investment-LLC/statemint.git"
  },
  "keywords": [
    "Cloud",
    "Resource",
    "AWS",
    "Azure"
  ],
  "author": "KRI LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KR-Investment-LLC/statemint/issues"
  },
  "homepage": "https://github.com/KR-Investment-LLC/statemint#readme",
  "dependencies": {
    "@types/jest": "^30.0.0",
    "axios": "^1.12.2",
    "commander": "^14.0.1",
    "moment": "^2.30.1",
    "prompts": "^2.4.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "@types/uuid": "^11.0.0",
    "fast-glob": "^3.3.3",
    "jest": "^30.1.3",
    "jest-junit": "^16.0.0",
    "shx": "^0.4.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
